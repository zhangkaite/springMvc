package com.zkt.find.goods.entity;

import java.util.ArrayList;
import java.util.List;

/** 发现实体 */
public class FindEntity {

	private String id;
	/** 发布人id */
	private String user_id;
	/** 发现描述 */
	private String find_desc;
	/** 发现标签组 */
	private String find_lable;
	/** 发现商品属性 */
	private String find_attribute;
	/** 发现发布时间 */
	private String publish_time;
	/** 用户想要数 */
	private String user_want_count;
	/** 用户评论数 */
	private String user_comment_count;
	/** 用户昵称 */
	private String nickname;
	/** 用户头像url */
	private String user_pic;
	/** 用户个性化签名 */
	private String introduction;

	/** 用户评论数组 */
	private List<FindCommentsEntity> findComents = new ArrayList<FindCommentsEntity>();
	/** 发表图片数组 */
	private List<FindPicEntity> findPic = new ArrayList<FindPicEntity>();
	/** 想要用户数组 */
	private List<FindWantUserEntity> wantUser = new ArrayList<FindWantUserEntity>();
	/** 用户选则达人标签数组 */
	private List<UserLableEntity> userLable = new ArrayList<UserLableEntity>();

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public String getFind_desc() {
		return find_desc;
	}

	public void setFind_desc(String find_desc) {
		this.find_desc = find_desc;
	}

	public String getFind_lable() {
		return find_lable;
	}

	public void setFind_lable(String find_lable) {
		this.find_lable = find_lable;
	}

	public String getFind_attribute() {
		return find_attribute;
	}

	public void setFind_attribute(String find_attribute) {
		this.find_attribute = find_attribute;
	}

	public String getPublish_time() {
		return publish_time;
	}

	public void setPublish_time(String publish_time) {
		this.publish_time = publish_time;
	}

	public String getUser_want_count() {
		return user_want_count;
	}

	public void setUser_want_count(String user_want_count) {
		this.user_want_count = user_want_count;
	}

	public String getUser_comment_count() {
		return user_comment_count;
	}

	public void setUser_comment_count(String user_comment_count) {
		this.user_comment_count = user_comment_count;
	}

	public List<FindCommentsEntity> getFindComents() {
		return findComents;
	}

	public void setFindComents(List<FindCommentsEntity> findComents) {
		this.findComents = findComents;
	}

	public List<FindPicEntity> getFindPic() {
		return findPic;
	}

	public void setFindPic(List<FindPicEntity> findPic) {
		this.findPic = findPic;
	}

	public List<FindWantUserEntity> getWantUser() {
		return wantUser;
	}

	public void setWantUser(List<FindWantUserEntity> wantUser) {
		this.wantUser = wantUser;
	}

	public List<UserLableEntity> getUserLable() {
		return userLable;
	}

	public void setUserLable(List<UserLableEntity> userLable) {
		this.userLable = userLable;
	}

	public String getNickname() {
		return nickname;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getUser_pic() {
		return user_pic;
	}

	public void setUser_pic(String user_pic) {
		this.user_pic = user_pic;
	}

	public String getIntroduction() {
		return introduction;
	}

	public void setIntroduction(String introduction) {
		this.introduction = introduction;
	}
	
	

}
